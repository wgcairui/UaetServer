{
  "name": "uartserver",
  "version": "2.0.0",
  "description": "LADS 物联网透传云平台",
  "author": "wgcairui",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_PORT=9002 ts-node --files ./server/index.ts",
    "dev:9010": "cross-env NODE_PORT=9010 ts-node --files ./server/index.ts",
    "start": "node ./server/index.js",
    "build:docker": "tsc; sudo docker build -t uartserver . && rm server/*.js && rm server/*/*.js && rm server/*/*/*.js",
    "run:docker": "sudo docker stop uartserver && sudo docker rm uartserver && sudo docker run -itd --name uartserver --restart always --init --network net -p 9010:9010 -v /etc/localtime:/etc/localtime uartserver"
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.7.9",
    "@koa/cors": "^3.1.0",
    "apollo-server-koa": "^2.19.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "consola": "^2.15.0",
    "crc": "^3.8.0",
    "cron": "^1.8.2",
    "cross-env": "^7.0.2",
    "crypto-js": "^4.0.0",
    "deepmerge": "^4.2.2",
    "graphql": "^15.3.0",
    "graphql-type-json": "^0.3.2",
    "ieee754": "^1.1.13",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^9.4.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.10.0",
    "nodemailer": "^6.4.11",
    "sha1": "^1.1.1",
    "socket.io": "^4.0.1",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/crc": "^3.4.0",
    "@types/cron": "^1.7.2",
    "@types/crypto-js": "^3.1.45",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/koa": "^2.11.4",
    "@types/koa-compress": "^4.0.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.0",
    "@types/koa-sslify": "^4.0.1",
    "@types/lodash": "^4.14.150",
    "@types/node": "^14.6.0",
    "@types/nodemailer": "^6.4.0",
    "@types/sha1": "^1.1.2",
    "apollo-server-types": "^0.4.0"
  }
}
