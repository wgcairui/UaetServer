{
  "name": "uartserver",
  "version": "2.5.1",
  "description": "LADS 物联网透传云平台",
  "author": {
    "name": "cr",
    "email": "wgcairui@icloud.com",
    "url": "https://github.com/wgcairui/UartServer.git"
  },
  "homepage": "https://uart.ladishb.com",
  "main": "server/app.ts",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_PORT=9002 ts-node --files ./server/app.ts",
    "dev:9010": "cross-env NODE_PORT=9010 ts-node --files ./server/app.ts",
    "start": "node ./server/app.js",
    "build:docker": "cross-env NODE_ENV=development && tsc; sudo docker build -t uartserver . && rm server/*.js && rm server/*/*.js && rm server/*/*/*.js",
    "run:docker": "sudo docker stop uartserver && sudo docker rm uartserver && sudo docker run -itd --name uartserver --restart always --init --network net -p 9010:9010 -v /etc/localtime:/etc/localtime uartserver"
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.7.9",
    "@koa/cors": "^3.1.0",
