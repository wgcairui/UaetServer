    "bcryptjs": "^2.4.3",
    "consola": "^2.15.0",
    "crc": "^3.8.0",
    "cron": "^1.8.2",
    "cross-env": "^7.0.2",
    "crypto-js": "^4.0.0",
    "deepmerge": "^4.2.2",
    "graphql": "^15.3.0",
    "graphql-type-json": "^0.3.2",
    "ieee754": "^1.1.13",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^9.4.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.10.0",
    "nodemailer": "^6.4.11",
    "sha1": "^1.1.1",
    "socket.io": "^4.1.2",
    "ws": "^7.4.5",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/crc": "^3.4.0",
    "@types/cron": "^1.7.2",
    "@types/crypto-js": "^3.1.45",
    "@types/echarts": "^4.9.7",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/koa": "^2.11.4",
